+		break;
+	default:
+		return 0;
+		break;
+	}
+
+	len = addrtot(&sub->addr, format, dst, dstlen);
+	if (len < dstlen) {
+		dst[len - 1] = '/';
+		p = dst + len;
+		rest = dstlen - len;
+	} else {
+		p = NULL;
+		rest = 0;
+	}
+
+
+	len += ultoa((unsigned long)sub->maskbits, 10, p, rest);
+
+	return len;
+}
diff -Nur linux/net/ipsec/libopenswan/subnettypeof.c linux.ipsec/net/ipsec/libopenswan/subnettypeof.c
--- linux/net/ipsec/libopenswan/subnettypeof.c	Thu Jan  1 01:00:00 1970
+++ linux.ipsec/net/ipsec/libopenswan/subnettypeof.c	Fri Mar 18 12:06:10 2005
@@ -0,0 +1,109 @@
+/*
+ * extract parts of an ip_subnet, and related
+ * Copyright (C) 2000  Henry Spencer.
+ * 
+ * This library is free software; you can redistribute it and/or modify it
+ * under the terms of the GNU Library General Public License as published by
