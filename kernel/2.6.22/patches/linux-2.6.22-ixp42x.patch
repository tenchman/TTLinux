+    strlcpy(t->u.cmdline.cmdline + strlen(avila_rtc_probe), p,
+        COMMAND_LINE_SIZE - strlen(avila_rtc_probe));
+
+    /* Terminate the table. */
+    t = tag_next(t);
+    t->hdr.tag = ATAG_NONE;
+    t->hdr.size = 0;
+}
